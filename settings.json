{
    /*
        üå≥ Git
    */

    "git.openRepositoryInParentFolders": "always",
    "git.terminalAuthentication": true,
    "git.mergeEditor": false,
    "diffEditor.wordWrap": "off",

    /*
        ‚öôÔ∏è Core
    */

    "liveServer.settings.donotShowInfoMsg": true,
    "update.showReleaseNotes": true,
    "extensions.autoUpdate": true,
    "update.mode": "none",

    /*
        üìÅ Explorer
    */

    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,

    "settings-repository.branch": "main",
    "settings-repository.autoSync": true,
    "settings-repository.repository": "https://github.com/luisnquin/vscode-settings.git",
    "settings-repository.includeHostnameInCommitMessage": true,

    /*
        üìü Integrated terminal
    */

    // "terminal.integrated.fontFamily": "'Monocraft', 'Symbols Nerd Font Mono'",

    "terminal.integrated.fontFamily": "'Cascadia Code', 'Symbols Nerd Font Mono'",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.gpuAcceleration": "on",
    "terminal.integrated.scrollback": 10000,
    "terminal.integrated.inheritEnv": true,
    "terminal.integrated.fontSize": 14,

    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "bash",
            "icon": "terminal-bash"
        },

        "zsh": {
            "path": "zsh"
        },

        "fish": {
            "path": "fish"
        },

        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        }
    },

    /*
        ü™ü Window settings
    */

    "window.title": "${dirty}${rootName}/.../${activeEditorShort} ${separator} VSCode",
    "window.openFoldersInNewWindow": "on",
    "window.customMenuBarAltFocus": true,
    "window.openFilesInNewWindow": "off",
    "window.titleBarStyle": "custom",
    "window.dialogStyle": "custom",
    "window.confirmBeforeClose": "never",
    "window.doubleClickIconToClose": true,
    "window.menuBarVisibility": "compact",
    "window.commandCenter": true,

    /*
        üõ†Ô∏è Workbench
    */

    "workbench.editorLargeFileConfirmation": 1024,
    "workbench.editor.pinnedTabSizing": "compact",
    "workbench.iconTheme": "eq-material-theme-icons-ocean",
    "workbench.panel.opensMaximized": "never",
    "workbench.editor.labelFormat": "short",
    "workbench.sideBar.location": "right",
    "workbench.statusBar.visible": false,
    "workbench.settings.editor": "json",
    "workbench.editor.showIcons": true,
    "workbench.editor.showTabs": "multiple",
    "workbench.startupEditor": "none",

    "files.autoSaveDelay": 1000,
    "files.autoSave": "afterDelay",
    "files.refactoring.autoSave": true,
    "files.associations": {
        "example.json": "jsonc",
        "*.theme": "plaintext",
        ".firebaserc": "json",
        ".babelrc": "json",
        "*.yml": "yaml",
        "*.yaml": "yaml",
        ".env.example": "env",
        ".env.production": "env"
    },
    "files.exclude": {
        "node_modules": true,
        ".direnv": true,
        "**/.next": true,
        "**/coverage-ts": true
    },
    // "json.schemas": [
    //     {
    //         "url": "https://ecs-intellisense.s3-us-west-2.amazonaws.com/task-definition/schema.json",
    //         "fileMatch": [".aws/task-definition.json"],
    //         "schema": true
    //     }
    // ],

    /*
        üìù Editor settings
    */

    "editor.tokenColorCustomizations": {
        "[One Dark Pro]": {
            "textMateRules": [
                {
                    "scope": [
                        "punctuation.definition.sequence.start.yaml",
                        "punctuation.definition.sequence.end.yaml",
                        "punctuation.definition.dictionary.begin.json.comments",
                        "meta.structure.dictionary.json.comments",
                        "meta.structure.array.json.comments",
                        "meta.structure.dictionary.value.json.comments",
                        "meta.structure.dictionary.json.comments",
                        "meta.structure.dictionary.value.json.comments",
                        "meta.structure.dictionary.json.comments",
                        "meta.structure.dictionary.value.json.comments",
                        "meta.structure.dictionary.json.comments"
                    ],
                    "settings": {
                        "foreground": "#ADBAC7"
                    }
                },

                {
                    "scope": [
                        "entity.name.tag.yaml",
                        "support.type.property-name.json.comments",
                        "punctuation.support.type.property-name.begin.json.comments",
                        "punctuation.support.type.property-name.end.json.comments"
                    ],
                    "settings": {
                        "foreground": "#8DDB8C"
                    }
                },

                {
                    "scope": [
                        "string.quoted.double.json.comments",
                        "string.unquoted.plain.out.yaml",
                        "string.unquoted.plain.out.yaml",
                        "string.quoted.double.yaml",
                        "string.quoted.single.yaml"
                    ],
                    "settings": {
                        "foreground": "#8cccf7"
                    }
                },

                {
                    "scope": "constant",
                    "settings": {
                        "foreground": "#d1bd66"
                    }
                }
            ]
        },

        "[Tokyo Night]": {
            "textMateRules": [
                {
                    "scope": "variable.other.declaration.go",
                    "settings": {
                        "foreground": "#cce6eb"
                    }
                },
                {
                    "scope": "constant.language",
                    "settings": {
                        "foreground": "#d1bd66"
                    }
                },
                {
                    "scope": "entity.name",
                    "settings": {
                        "foreground": "#73c9df"
                    }
                },
                {
                    "scope": "string",
                    "settings": {
                        "foreground": "#8df89b"
                    }
                }
            ]
        }
    },

    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "rules": {
            "*.mutable": {
                "underline": false
            }
        },
        "[One Dark Pro]": {
            "rules": {},

            "enabled": true
        }
    },

    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.fontFamily": "'Cascadia Code', 'Symbols Nerd Font Mono'",
    "editor.suggest.selectionMode": "whenQuickSuggestion",
    "editor.guides.highlightActiveIndentation": false,
    "editor.guides.highlightActiveBracketPair": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.autoClosingBrackets": "languageDefined",
    "editor.guides.bracketPairsHorizontal": false,
    "editor.linkedEditing": true,

    // "editor.guides.bracketPairs": true,

    "editor.scrollbar.verticalScrollbarSize": 13,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.hideCursorInOverviewRuler": true,
    "editor.largeFileOptimizations": false,
    "editor.scrollbar.horizontal": "auto",
    "editor.scrollbar.vertical": "hidden",
    "editor.renderWhitespace": "boundary",
    "editor.inlineSuggest.enabled": true,
    "editor.stickyScroll.enabled": true,
    "editor.overviewRulerBorder": false,
    "editor.guides.indentation": false,
    "editor.fastScrollSensitivity": 50,
    "editor.guides.bracketPairs": true,
    "editor.formatOnSaveMode": "file",
    "editor.cursorBlinking": "phase",
    "editor.indentSize": "tabSize",
    "editor.glyphMargin": false,
    "editor.smoothScrolling": false,
    "editor.autoIndent": "advanced",
    "editor.matchBrackets": "never",
    "editor.formatOnPaste": false,
    "editor.formatOnType": false,
    "editor.stickyTabStops": true,
    "editor.tabCompletion": "on",
    "editor.fontLigatures": true,
    "editor.formatOnSave": true,
    "editor.cursorStyle": "line",
    "editor.letterSpacing": 0,
    "editor.wordWrap": "off",
    "editor.cursorWidth": 2,
    "editor.tabSize": 4,

    "zenMode.centerLayout": false,
    "breadcrumbs.enabled": true,

    "material-icon-theme.opacity": 1,
    "material-icon-theme.folders.color": "#83aff2",
    "material-icon-theme.folders.theme": "specific",
    "material-icon-theme.files.associations": {
        ".golangci.yaml": "Go_gopher",
        "repository.go": "Codecov",
