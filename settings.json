    "indenticator.width": 0.1,

    /* 
        üß™ Programming languages and other tools
    */

    // Go

    "gopls": {
        "formatting.gofumpt": true
    },
    "go.useLanguageServer": true,
    "go.toolsManagement.autoUpdate": true,
    "go.lintTool": "golangci-lint",
    "go.diagnostic.vulncheck": "Imports",
    "go.formatTool": "gofumpt",
    "go.lintOnSave": "package",
    "go.survey.prompt": true,
    "go.inlayHints.constantValues": false,
    "go.inlayHints.functionTypeParameters": false,
    "go.autocompleteUnimportedPackages": true,
    "go.liveErrors": {
        "enabled": false,
        "delay": 500
    },
    "go.lintFlags": ["--fast"],

    "[go]": {
        "editor.insertSpaces": false,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
        "editor.tabSize": 4,
        "editor.suggest.snippetsPreventQuickSuggestions": false
    },

    //  Nix

    "[nix]": {
        "editor.defaultFormatter": "kamadorueda.alejandra",
        "editor.insertSpaces": true,
        "editor.tabSize": 2
    },

    "nix.enableLanguageServer": true,
    "nix.serverPath": "rnix-lsp",
    "alejandra.program": "alejandra",

    "[ocaml]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true,
        "editor.defaultFormatter": "ocamllabs.ocaml-platform"
    },

    "ocaml.dune.autoDetect": true,

    // Rust
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },

    "rust-analyzer.inlayHints.closingBraceHints.enable": false,

    // Shell script

    "[shellscript]": {
        "editor.defaultFormatter": "mkhl.shfmt"
    },

    // JSON, and JavaScript related

    "typescript.updateImportsOnFileMove.enabled": "always",

    "eslint.trace.server": "off",

    "prettier.documentSelectors": ["**/*.astro"],
    "prettier.trailingComma": "none",
    "prettier.singleQuote": true,
    "prettier.endOfLine": "auto",
    "prettier.useTabs": false,
    "prettier.printWidth": 120,
    "prettier.enable": true,
    "prettier.tabWidth": 4,
    "prettier.semi": false,

    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.autoIndent": "full"
    },

    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.autoIndent": "full"
    },

    "reactSnippets.settings.prettierEnabled": true,

    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

    "[javascriptreact]": {
        "editor.rulers": [125],

        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

    "[typescript]": {
        "editor.rulers": [125],

        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

    "[typescriptreact]": {
        "editor.rulers": [125],

        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

    // Python
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "pylint.args": ["--disable=C0111,C0114,C0115,C0116"],

    "python.terminal.activateEnvironment": true,
    "python.languageServer": "Default",

    // Dart
    "[dart]": {
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off",
        "editor.selectionHighlight": false,
        "editor.suggestSelection": "first",
        "editor.rulers": [80]
    },

    // Web-only

    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },

    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },

    "[scss]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },

    // Latex
    "latex-workshop.latex.watch.pdf.delay": 250,
    "latex-workshop.latex.autoClean.run": "onSucceeded",
    "latex-workshop.latex.autoBuild.run": "onFileChange",

    // Etc languages
    "[markdown]": {
        "editor.wordWrap": "off",
        "editor.quickSuggestions": {
            "comments": true,
            "strings": true,
            "other": true
        },
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },

    "[toml]": {
        "editor.defaultFormatter": "tamasfe.even-better-toml"
    },

    "[just]": {
        "editor.defaultFormatter": "skellock.just"
    },

    "[vue]": {
        "editor.defaultFormatter": "rvest.vs-code-prettier-eslint"
    },

    "[vue-directives]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },

    "volar.format.initialIndent": {
        "html": true
    },

    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.tabSize": 2,
        "editor.autoIndent": "keep"
    },
    "yaml.format.enable": true,
    "yaml.completion": true,

    "[makefile]": {
        "editor.insertSpaces": false,
        "editor.tabCompletion": "on",
        "editor.detectIndentation": true
    },

