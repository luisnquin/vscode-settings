    "editor.tokenColorCustomizations": {
        "[Default Dark Modern]": {
            "textMateRules": [
                {
                    "scope": [
                        "variable",
                        "variable.declaration.hcl",
                        "eta.definition.variable.name",
                        "support.variable",
                        "entity.name.variable",
                        "constant.other.placeholder"
                    ],
                    "settings": {
                        "foreground": "#4d3c3c"
                    }
                },
                {
                    "scope": [
                        "entity.name.type",
                        "keyword.other.interpolation.begin.hcl",
                        "keyword.other.interpolation.end.hcl",
                        "keyword.declaration.provider.terragrunt",
                        "storage.modifier.version.terragrunt",
                        "keyword"
                    ],
                    "settings": {
                        "foreground": "#a3a399",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": [
                        "variable",
                        "punctuation.separator.key-value.mapping.yaml",
                        "entity.name.tag",
                        "entity.name.function",
                        "support.type.property-name"
                    ],
                    "settings": {
                        "foreground": "#dadacf"
                    }
                },
                {
                    "scope": [
                        "punctuation.definition.sequence.start.yaml",
                        "comment.block.json.comments",
                        "comment",
                        "punctuation.definition.sequence.end.yaml",
                        "punctuation.definition.dictionary.begin.json.comments",
                        "meta.structure.dictionary.json.comments",
                        "meta.structure.array.json.comments",
                        "meta.structure.dictionary.value.json.comments",
                        "meta.structure.dictionary.json.comments",
                        "meta.structure.dictionary.value.json.comments",
                        "meta.structure.dictionary.json.comments",
                        "meta.structure.dictionary.value.json.comments",
                        "meta.structure.dictionary.json.comments",
                        "punctuation.definition.string.begin.hcl"
                    ],
                    "settings": {
                        "foreground": "#767777"
                    }
                },
                {
                    "scope": ["keyword.control", "constant.language"],
                    "settings": {
                        "foreground": "#ad2c53"
                    }
                },
                {
                    "scope": [
                        "string.quoted.double",
                        "punctuation.definition.string.begin.hcl",
                        "variable.other.enummember",
                        "punctuation.definition.string.end.hcl",
                        "string"
                    ],
                    "settings": {
                        "foreground": "#eb2f67"
                    }
                }
            ]
        },

        "[One Dark Pro]": {
            "textMateRules": [
                {
                    "scope": [
                        "punctuation.definition.sequence.start.yaml",
                        "punctuation.definition.sequence.end.yaml",
                        "punctuation.definition.dictionary.begin.json.comments",
                        "meta.structure.dictionary.json.comments",
                        "meta.structure.array.json.comments",
                        "meta.structure.dictionary.value.json.comments",
                        "meta.structure.dictionary.json.comments",
                        "meta.structure.dictionary.value.json.comments",
                        "meta.structure.dictionary.json.comments",
                        "meta.structure.dictionary.value.json.comments",
                        "meta.structure.dictionary.json.comments"
                    ],
                    "settings": {
                        "foreground": "#ADBAC7"
                    }
                },

                {
                    "scope": [
                        "entity.name.tag.yaml",
                        "support.type.property-name.json.comments",
                        "punctuation.support.type.property-name.begin.json.comments",
                        "punctuation.support.type.property-name.end.json.comments"
                    ],
                    "settings": {
                        "foreground": "#8DDB8C"
                    }
                },

                {
                    "scope": [
                        "string.quoted.double.json.comments",
                        "string.unquoted.plain.out.yaml",
                        "string.unquoted.plain.out.yaml",
                        "string.quoted.double.yaml",
                        "string.quoted.single.yaml"
                    ],
                    "settings": {
                        "foreground": "#8cccf7"
                    }
                },

                {
                    "scope": "constant",
                    "settings": {
                        "foreground": "#d1bd66"
                    }
                }
            ]
        },

        "[Tokyo Night]": {
            "textMateRules": [
                {
                    "scope": "variable.other.declaration.go",
                    "settings": {
                        "foreground": "#cce6eb"
                    }
                },
                {
                    "scope": "constant.language",
                    "settings": {
                        "foreground": "#d1bd66"
                    }
                },
                {
                    "scope": "entity.name",
                    "settings": {
                        "foreground": "#73c9df"
                    }
                },
                {
                    "scope": "string",
                    "settings": {
                        "foreground": "#8df89b"
                    }
                }
            ]
        }
    },

